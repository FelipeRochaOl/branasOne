{
  "name": "branas",
  "version": "1.0.0",
  "description": "Curse Clean Code and Clean Architecture",
  "main": "index.ts",
  "author": "Felipe Rocha",
  "license": "MIT",
  "scripts": {
    "test": "npx jest --colors --coverage --verbose --silent --runInBand --injectGlobals false /test",
    "test:dev": "npx jest --colors --coverage --verbose --silent --runInBand --watchAll --injectGlobals false /test",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --colors --coverage --verbose --runInBand --watchAll --injectGlobals false /test",
    "coverage": "npx jest --colors --coverage --verbose --silent --runInBand --injectGlobals false /test",
    "dev": "ts-node-dev -r tsconfig-paths/register src/main.ts"
  },
  "dependencies": {
    "ts-node": "^10.9.2",
    "typescript": "*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.7.0",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.1.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
